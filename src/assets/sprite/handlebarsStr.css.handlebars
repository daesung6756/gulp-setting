{{#spritesheet}}
    .sp {
    display: block;
    flex-shrink: 0;
    background-image: url($sprite-url);
    background-repeat: no-repeat;
    background-size: {{px.width}} {{px.height}};
    }
{{/spritesheet}}

{{#sprites}}
    .sp-{{name}} {
    width: {{px.width}};
    height: {{px.height}};
    background-position: {{px.offset_x}} {{px.offset_y}};
    }
{{/sprites}}